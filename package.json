{
  "name": "indoor-garden",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev-no-emulators": "yarn && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev": "concurrently -n emulators,dev-server \"yarn emulators\" \"yarn dev-no-emulators\"",
